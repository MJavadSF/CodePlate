{
  "name": "cra-template-cp",
  "version": "1.0.4",
  "license": "MIT",
  "main": "template.json",
  "description": "ReactJS Custom Boilerplate",
  "keywords": [
    "react",
    "create-react-app",
    "cra-template",
    "template",
    "eslint",
    "redux",
    "react-redux",
    "react-router",
    "react-router-dom",
    "typescript",
    "prettier"
  ],
  "author": "MJ. Soleymani Fard <mjavadsf72@gmail.com> (https://imjavad.ir)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MJavadSF/CodePlate.git"
  },
  "bugs": {
    "url": "https://github.com/MJavadSF/CodePlate/issues",
    "email": "mjavadsf72@gmail.com"
  },
  "config": {
    "port": "9090"
  },
  "scripts": {
    "start": "nodemon --exec react-scripts start",
    "prebuild": "yarn setver && yarn build:clean npm shrinkwrap",
    "build": "GENERATE_SOURCEMAP=false react-scripts build ",
    "build:clean": "rimraf ./build",
    "postbuild": "compress-cra",
    "setver": "node ./internals/version/version.ts",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:generators": "ts-node ./internals/testing/generators/test-generators.ts",
    "start:prod": "yarn run build && serve -s build",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext ts,tsx",
    "lint": "yarn run eslint src",
    "lint:fix": "yarn eslint --fix src && yarn lint --fix",
    "lint:css": "stylelint src/**/*.{css,less,scss,sss}",
    "lint:staged": "lint-staged",
    "generate": "plop --plopfile internals/generators/plopfile.ts",
    "prettify": "prettier --write \"src/**/*.{js,jsx,json,md,ts,tsx,css,less}\"",
    "precommit": "pretty-quick --staged",
    "extract-messages": "i18next-scanner --config=internals/extractMessages/i18next-scanner.config.js",
    "format": "prettier --write \"**/*.{js,jsx,json,md,ts,tsx,css,less,sass}\"",
    "prepare": "husky install"
  },
  "standard-version": {
    "skip": {
      "commit": true
    }
  },
  "nodemonConfig": {
    "delay": 2500
  },
  "cpu": [
    "x64",
    "x32",
    "ia32",
    "arm",
    "arm64",
    "mips"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 2 safari version",
      "last 2 edge versions"
    ],
    "modern": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  },
  "engines": {
    "yarn": ">=1.22",
    "node": ">=16",
    "npm": ">=8"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/messages.ts",
      "!src/**/*/types.ts",
      "!src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "eslintConfig": {
    "extends": "react-app",
    "ignorePatterns": [
      "node_modules/"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/fontfaceobserver": "^2.1.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^18.0.0",
    "@types/shelljs": "^0.8.11",
    "@types/styled-components": "^5.1.25",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "acorn": "6.4.2",
    "acorn-dynamic-import": "4.0.0",
    "ahooks": "^3.5.0",
    "buffer": "6.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.18.0",
    "fontfaceobserver": "^2.3.0",
    "i18next": "^21.8.10",
    "i18next-browser-languagedetector": "^6.1.4",
    "i18next-scanner": "^3.3.0",
    "inquirer": "^7.3.3",
    "inquirer-directory": "^2.2.0",
    "jest-styled-components": "^7.0.8",
    "node-plop": "0.31.0",
    "plop": " 2.7.6",
    "precompress": "7.0.1",
    "prettier": "2.7.1",
    "prop-types": "15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "1.3.0",
    "react-i18next": "^11.17.2",
    "react-icons": "4.4.0",
    "react-is": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-test-renderer": "^18.2.0",
    "react-use-image": "0.2.3",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@babel/core": "7.18.5",
    "@babel/plugin-syntax-flow": "7.17.12",
    "@babel/plugin-transform-react-jsx": "7.17.12",
    "@babel/preset-env": "7.18.2",
    "@gfx/zopfli": "^1.0.15",
    "@types/jest": "^28.1.3",
    "@types/node": "18.0.0",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/parser": "5.29.0",
    "babel-eslint": "^10.1.0",
    "compress-create-react-app": "1.2.1",
    "customize-cra": "1.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "^8.0.1",
    "less": "4.1.3",
    "lint-staged": "13.0.2",
    "nodemon": "2.0.18",
    "pretty-quick": "^3.1.3",
    "react-app-polyfill": "^3.0.0",
    "rimraf": "^3.0.2",
    "shelljs": "0.8.5",
    "standard-version": "9.5.0",
    "stylelint": "14.9.1",
    "stylelint-config-recommended": "^8.0.0",
    "stylelint-config-standard": "26.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-csstree-validator": "2.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "stylelint-scss": "^4.2.0",
    "stylus": "^0.58.1",
    "ts-node": "10.8.1",
    "typescript": "^4.7.4",
    "uglifyify": "^5.0.2",
    "web-vitals": "^2.1.4"
  }
}
